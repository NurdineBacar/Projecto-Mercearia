
public class Pessoa {
	private String nome, idade,genero;
	Pessoa pai;
	Pessoa mae;
	
	public Pessoa(String nome,String idade, String genero) {
		this.nome=nome;
		this.idade=idade;
		this.genero=genero;
	}

	
	public String getNome() {
		return nome;
	}


	public void setNome(String nome) {
		this.nome = nome;
	}


	public String getIdade() {
		return idade;
	}


	public void setIdade(String idade) {
		this.idade = idade;
	}


	public String getGenero() {
		return genero;
	}


	public void setGenero(String genero) {
		this.genero = genero;
	}


	public Pessoa getPai() {
		return pai;
	}


	public void setPai(Pessoa pai) {
		this.pai = pai;
	}


	public Pessoa getMae() {
		return mae;
	}


	public void setMae(Pessoa mae) {
		this.mae = mae;
	}


	public Pessoa(String nome, String idade, String genero, String pai, String mae) {
		super();
		this.nome = nome;
		this.idade = idade;
		this.genero = genero;
		this.pai.setNome(nome);
		this.mae.setNome(mae);
	}
	
	public String toString() {
		return "Pessoa [nome=" + nome + ", idade=" + idade + ", genero=" + genero + ", pai=" + pai + ", mae=" + mae
				+ "]";
	}
	
	
	
	
	
}
