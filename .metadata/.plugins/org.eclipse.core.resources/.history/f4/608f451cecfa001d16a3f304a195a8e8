import java.awt.Color;
import java.awt.Graphics;
import java.awt.Polygon;

import javax.swing.JPanel;

public class Painel1 extends JPanel {

	public Painel1() {
		this.setSize(500,500);
		this.setVisible(true);
		
	}
	
	
	protected void paintComponent(Graphics g) {
		super.paintComponent(g);
		g.setColor(Color.green);
		g.drawArc(85, 50, 300, 300, 0, 360);
		g.drawArc(130, 120, 70, 70, 0, 360);
		g.fillArc(147, 139, 35, 35, 0, 360);
		g.drawArc(265, 120, 70, 70, 0, 360);
		g.fillArc(283, 139, 35, 35, 0, 360);
		g.drawArc(125, 110, 220, 190, -30, -120);
		g.drawLine(200, 240, 230, 190);
		g.drawLine(230, 190, 260, 240);
	}
}
