import java.awt.Color;
import java.awt.Graphics;
import java.awt.Polygon;

import javax.swing.JPanel;

public class Painel1 extends JPanel {

	public Painel1() {
		
	}
	
	
	protected void paintComponent(Graphics g) {
		super.paintComponent(g);
		g.setColor(Color.green);
		g.drawArc(85, 100, 300, 300, 0, 360);
		g.drawArc(130, 170, 70, 70, 0, 360);
		g.fillArc(147, 189, 35, 35, 0, 360);
		g.drawArc(265, 170, 70, 70, 0, 360);
		g.fillArc(283, 189, 35, 35, 0, 360);
		g.drawArc(125, 160, 220, 190, -30, -120);
		g.drawLine(200, 290, 230, 190);
		g.drawLine(230, 240, 260, 240);
	}
}
